{
  "name": "Tab Mover",
  "manifest_version": 3,
  "author": "Neo Genesis",
  "version": "1.1.0",
  "description": "Move tabs with keyboard shortcuts.",
  "permissions": ["tabs"],
  "action": {
    "default_icon": "icon-128.png",
    "default_title": "Move tabs with keyboard shortcuts."
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "move-tab-end": {
      "description": "Move Tab to End",
      "suggested_key": {
        "default": "Ctrl+Shift+Down",
        "mac": "MacCtrl+Shift+Down"
      }
    },
    "move-tab-left": {
      "description": "Move Tab to Left",
      "suggested_key": {
        "default": "Ctrl+Shift+Left",
        "mac": "MacCtrl+Shift+Left"
      }
    },
    "move-tab-right": {
      "description": "Move Tab to Right",
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "MacCtrl+Shift+Right"
      }
    },
    "move-tab-start": {
      "description": "Move Tab to Start",
      "suggested_key": {
        "default": "Ctrl+Shift+Up",
        "mac": "MacCtrl+Shift+Up"
      }
    }
  },
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png",
    "16": "icon-16.png"
  }
}
